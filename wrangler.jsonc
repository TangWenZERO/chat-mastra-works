{
  // Cloudflare Worker 项目名称
  "name": "chat-mastra-works",
  // Worker 入口文件路径
  "main": ".mastra/output/index.mjs",
  // 指定使用的 Cloudflare 平台兼容性日期
  "compatibility_date": "2024-09-23",
  // 需要启用的兼容性标志列表
  "compatibility_flags": [
    // 启用 Node.js API 兼容层
    "nodejs_compat",
    // 在运行时自动填充 process.env 变量
    "nodejs_compat_populate_process_env"
  ],
  // 是否为 workers.dev 提供预览访问
  "workers_dev": true,
  // 默认环境变量配置
  "vars": {
    // 默认环境标识
    "ENVIRONMENT": "production",
    // OpenAI API 密钥占位符
    "OPENAI_API_KEY": "you openai api key"
  },
  // 本地构建相关配置
  "build": {
    // 构建命令
    "command": "npm run build",
    // 需要监听变更的目录
    "watch_dir": ".mastra/output"
  },
  // 多环境单独配置
  "env": {
    // 开发环境配置
    "development": {
      // 开发环境的环境变量
      "vars": {
        // 开发环境标识
        "ENVIRONMENT": "development",
        // 开发环境的 OpenAI API 密钥占位符
        "OPENAI_API_KEY": "you openai api key"
      }
    },
    // 生产环境配置
    "production": {
      // 生产环境的环境变量
      "vars": {
        // 生产环境标识
        "ENVIRONMENT": "production",
        // 生产环境的 OpenAI API 密钥占位符
        "OPENAI_API_KEY": "you openai api key"
      }
    }
  }
}
